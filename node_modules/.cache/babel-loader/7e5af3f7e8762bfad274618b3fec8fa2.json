solute; left: 1190px; top: 0\">\n            <img class=\"toucher\" src=\"../../assets/icons/toucher.svg\" alt=\"\">\n          </div>\n          <div style=\"margin-top: 58px\" class=\"screensaver_footer_actions_text\">\n            <p class=\"screensaver_footer_actions_ru\">\n              Дотроньтесь, чтобы начать.\n            </p>\n            <p class=\"screensaver_footer_actions_en\">\n              Tap to Start\n            </p>\n          </div>\n        </div>\n      </div>\n\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from \"vuex\";\n\nexport default {\n  name: 'ScreenSaver',\n  components: {},\n  data(){\n    return {\n      firstPhotoClasses: ['screensaver_photos_1'],\n      secondPhotoClasses: ['screensaver_photos_2'],\n      thirdPhotoClasses: ['screensaver_photos_3'],\n      fourthPhotoClasses: ['screensaver_photos_4'],\n      fifthPhotoClasses: ['nodisplay'],\n      sixthPhotoClasses: ['nodisplay'],\n      seventhPhotoClasses: ['nodisplay'],\n      eighthPhotoClasses: ['nodisplay'],\n      timerId: null,\n      indx: 1\n    }\n  },\n  computed: {\n    ...mapGetters([\"SHOW_SCREENSAVER\"]),\n  },\n  methods: {\n    ...mapActions([\"CHANGE_SCREENSAVER_STATE\"]),\n\n    updateScreenSaver(){\n      this.CHANGE_SCREENSAVER_STATE(true)\n      this.indx = 1\n      this.firstPhotoClasses = ['screensaver_photos_1']\n      this.secondPhotoClasses = ['screensaver_photos_2']\n      this.thirdPhotoClasses = ['screensaver_photos_3']\n      this.fourthPhotoClasses = ['screensaver_photos_4']\n      this.fifthPhotoClasses = ['nodisplay']\n      this.sixthPhotoClasses = ['nodisplay']\n      this.seventhPhotoClasses = ['nodisplay']\n      this.eighthPhotoClasses = ['nodisplay']\n    }\n  },\n  watch: {\n    'SHOW_SCREENSAVER'(val){\n      if(val) {\n        this.timerId = setInterval(() => {\n          if(this.indx == 1) {\n            this.firstPhotoClasses.push('hide')\n            this.secondPhotoClasses.push('toFirst')\n            this.thirdPhotoClasses.push('toSecond')\n          } else if(this.indx == 2) {\n            this.firstPhotoClasses = ['nodisplay']\n            this.secondPhotoClasses = ['screensaver_photos_1']\n            this.thirdPhotoClasses = ['screensaver_photos_2']\n            this.fourthPhotoClasses = ['screensaver_photos_3']\n            this.fifthPhotoClasses = ['screensaver_photos_4']\n          } else if(this.indx == 3) {\n            this.secondPhotoClasses.push('hide')\n            this.thirdPhotoClasses.push('toFirst')\n            this.fourthPhotoClasses.push('toSecond')\n          } else if(this.indx == 4) {\n            this.secondPhotoClasses = ['nodisplay']\n            this.thirdPhotoClasses = ['screensaver_photos_1']\n            this.fourthPhotoClasses = ['screensaver_photos_2']\n            this.fifthPhotoClasses = ['screensaver_photos_3']\n            this.sixthPhotoClasses = ['screensaver_photos_4']\n          } else if(this.indx == 5) {\n            this.thirdPhotoClasses.push('hide')\n            this.fourthPhotoClasses.push('toFirst')\n            this.fifthPhotoClasses.push('toSecond')\n          } else if(this.indx == 6) {\n            this.thirdPhotoClasses = ['nodisplay']\n            this.fourthPhotoClasses = ['screensaver_photos_1']\n            this.fifthPhotoClasses = ['screensaver_photos_2']\n            this.sixthPhotoClasses = ['screensaver_photos_3']\n            this.seventhPhotoClasses = ['screensaver_photos_4']\n          } else if(this.indx == 7) {\n            this.fourthPhotoClasses.push('hide')\n            this.fifthPhotoClasses.push('toFirst')\n            this.sixthPhotoClasses.push('toSecond')\n          } else if(this.indx == 8) {\n            this.fourthPhotoClasses = ['nodisplay']\n            this.fifthPhotoClasses = ['screensaver_photos_1']\n            this.sixthPhotoClasses = ['screensaver_photos_2']\n            this.seventhPhotoClasses = ['screensaver_photos_3']\n            this.eighthPhotoClasses = ['screensaver_photos_4']\n          } else if(this.indx == 9) {\n            this.fifthPhotoClasses.push('hide')\n            this.sixthPhotoClasses.push('toFirst')\n            this.seventhPhotoClasses.push('toSecond')\n          } else if(this.indx == 10) {\n            this.fifthPhotoClasses = ['nodisplay']\n            this.sixthPhotoClasses = ['screensaver_photos_1']\n            this.seventhPhotoClasses = ['screensaver_photos_2']\n            this.eighthPhotoClasses = ['screensaver_photos_3']\n            this.firstPhotoClasses = ['screensaver_photos_4']\n          } else if(this.indx == 11) {\n            this.sixthPhotoClasses.push('hide')\n            this.seventhPhotoClasses.push('toFirst')\n            this.eighthPhotoClasses.push('toSecond')\n          } else if(this.indx == 12) {\n            this.sixthPhotoClasses = ['nodisplay']\n            this.seventhPhotoClasses = ['screensaver_photos_1']\n            this.eighthPhotoClasses = ['screensaver_photos_2']\n            this.firstPhotoClasses = ['screensaver_photos_3']\n            this.secondPhotoClasses = ['screensaver_photos_4']\n          } else if(this.indx == 13) {\n            this.seventhPhotoClasses.push('hide')\n            this.eighthPhotoClasses.push('toFirst')\n            this.firstPhotoClasses.push('toSecond')\n          } else if(this.indx == 14) {\n            this.seventhPhotoClasses = ['nodisplay']\n            this.eighthPhotoClasses = ['screensaver_photos_1']\n            this.firstPhotoClasses = ['screensaver_photos_2']\n            this.secondPhotoClasses = ['screensaver_photos_3']\n            this.thirdPhotoClasses = ['screensaver_photos_4']\n          } else if(this.indx == 15) {\n            this.eighthPhotoClasses.push('hide')\n            this.firstPhotoClasses.push('toFirst')\n            this.secondPhotoClasses.push('toSecond')\n          } else if(this.indx == 16) {\n            this.eighthPhotoClasses = ['nodisplay']\n            this.firstPhotoClasses = ['screensaver_photos_1']\n            this.secondPhotoClasses = ['screensaver_photos_2']\n            this.thirdPhotoClasses = ['screensaver_photos_3']\n            this.fourthPhotoClasses = ['screensaver_photos_4']\n            this.indx = 0\n          }\n          this.indx++\n        }, 2000)        \n      } else {\n        clearInterval(this.timerId)\n      }\n    }\n  }\n}\n</script>\n\n<style>\n.screensaver_wrapper{\n  position: fixed;\n  top: 0;\n  left: 0;\n  z-index: 20 !important;\n  overflow: auto;\n  width: 1920px;\n  height: 1080px;\n}\n\n.screensaver_wrapper::-webkit-scrollbar {\n  width: 0;\n}\n\n.screensaver_photos_1{\n  position: absolute;\n  top: 0;\n  left: 0;\n  z-index: 25;\n}\n\n.screensaver_photos_2{\n  position: absolute;\n  top: 0;\n  left: 165px;\n  z-index: 24;\n}\n\n.screensaver_photos_3{\n  position: absolute;\n  top: 0;\n  left: 330px;\n  z-index: 23;\n}\n\n.screensaver_photos_4{\n  position: absolute;\n  top: 0;\n  left: 330px;\n  z-index: 22;\n}\n\n.screensaver_footer{\n  position: fixed;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  color: #fff;\n  background: rgba(0, 0, 0, 0.9);\n  padding-left: 124px;\n  padding-right: 100px;\n  height: 192px;\n  display: flex;\n  justify-content: space-between;\n  z-index: 30;\n}\n\n.screensaver_footer_title{\n  margin-top: 45px;\n  margin-bottom: 19px;\n}\n\n.screensaver_footer_title_ru{\n  font-style: normal;\n  font-weight: 500;\n  font-size: 62px;\n  line-height: 74px;\n  color: #C4C2C0;\n}\n\n.screensaver_footer_title_en{\n  font-style: normal;\n  font-weight: 500;\n  font-size: 54.5px;\n  line-height: 65px;\n  color: #504F50;\n}\n\n.screensaver_footer_actions{\n  display: inline-flex;\n}\n\n.screensaver_footer_actions_ru{\n  font-style: normal;\n  font-weight: 500;\n  font-size: 42px;\n  line-height: 50px;\n  width: 500px;\n  color: #C7C7C7;\n}\n\n.screensaver_footer_actions_en{\n  font-style: normal;\n  font-weight: 500;\n  font-size: 33.5px;\n  line-height: 40px;\n  color: #4D4D4D;\n}\n\n.hide{\n  animation-duration: 2s;\n  animation-name: slideleft;\n}\n\n@keyframes slideleft {\n  from {\n    transform: translateX(0);\n  }\n\n  to {\n    transform: translateX(-100%);\n  }\n}\n\n.toFirst{\n  animation-duration: 2s;\n  animation-name: stayFirst;\n}\n\n@keyframes stayFirst {\n  from {\n    left: 165px;\n  }\n\n  to {\n    left: 0;\n  }\n}\n\n.toSecond{\n  animation-duration: 2s;\n  animation-name: staySecond;\n}\n\n@keyframes staySecond {\n  from {\n    left: 330px;\n  }\n\n  to {\n    left: 165px;\n  }\n}\n\n.nodisplay{\n  display: none;\n}\n\n.toucher{\n  width: 203px;\n  height: auto;\n  display: flex;\n  animation-duration: 8s;\n  animation-iteration-count: infinite;\n  animation-name: mooving;\n  animation-timing-function: linear;\n}\n\n@keyframes mooving {\n  0% {\n    opacity: 1;\n    width: 203px;\n    transform: translateY(100%);\n  }\n  5% {\n    width: 203px;\n    transform: translateY(0px);\n  }\n  15% {\n    width: 110px;\n    transform: translateY(0px);\n  }\n  20% {\n    width: 203px;\n    transform: translateY(0px);\n  }\n  25% { \n    opacity: 1;\n    transform: translateY(100%);\n  }\n  26% {\n    opacity: 0;\n  }\n  100% {\n    opacity: 0;\n  }\n}\n\n.screensaver_footer_actions_text{\n  position: absolute; \n  left: 1385px; \n  top: 0;\n  animation-duration: 8s;\n  animation-iteration-count: infinite;\n  animation-name: moovingtext;\n  animation-timing-function: linear;\n}\n\n@keyframes moovingtext {\n  0% {\n    opacity: 0;\n    transform: translateX(100%);\n  }\n  35% { \n    opacity: 1;\n    transform: translateX(0%);\n  }\n  85%{\n    opacity: 0;\n  }\n  100% {\n    opacity: 0;\n  }\n}\n</style>\n"]}]}