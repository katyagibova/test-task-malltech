emplate>\n\n<script>\nimport Footer from './views/Footer.vue'\nimport ScreenSaver from './components/AppUnit/ScreenSaver.vue';\nimport { mapGetters, mapActions } from \"vuex\";\n\nexport default {\n  components: {\n    Footer,\n    ScreenSaver\n  },\n  computed: {\n    ...mapGetters([\"SCREENSAVER_STATE\"]),\n  },\n  data(){\n    return {\n      timerId: null,\n      allImgs: ['../img/sequenceNamesBook.338b56cc.png', '../img/sequenceLiteratureBook.c9187ce6.png', '../img/sequencePerestroikaBook.eb8b3250.png', '../img/sequenceNamesBookEn.a51123ee.png', '../img/sequenceLiteratureBookEn.a9e31a33.png', '../img/sequencePerestroikaBookEn.7fa23e15.png']\n    }\n  },\n  watch: {\n    'SCREENSAVER_STATE'(val){\n      if(val) {\n        clearInterval(this.timerId)\n        this.CHANGE_SCREENSAVER_STATE(false)\n        this.CHANGE_SCREENSAVER_SHOWING(false)\n      } else {\n        this.timerId = setTimeout(() => {\n          this.CHANGE_SCREENSAVER_SHOWING(true)\n          this.$router.push({name: 'MainScreen'})\n        }, 180000)\n      }\n    },\n    $route(to, from) {\n        const routeDeep = ['/', '/team', '/judge', '/finish', '/noData', '/info']\n        const toDepth = routeDeep.indexOf(to.path)\n        const fromDepth = routeDeep.indexOf(from.path)\n        this.transitionName = toDepth > fromDepth ? 'slide-left' : 'slide-right'\n    }\n  },\n  created(){\n    this.timerId = setTimeout(() => {\n      this.CHANGE_SCREENSAVER_SHOWING(true)\n      this.$router.push({name: 'MainScreen'})\n    }, 180000)\n    let img1 = document.createElement('img');\n    img1.src = this.allImgs[0];\n    let img2 = document.createElement('img');\n    img2.src = this.allImgs[1];\n    let img3 = document.createElement('img');\n    img3.src = this.allImgs[2];\n    let img4 = document.createElement('img');\n    img4.src = this.allImgs[3];\n    let img5 = document.createElement('img');\n    img5.src = this.allImgs[4];\n    let img6 = document.createElement('img');\n    img6.src = this.allImgs[5];\n  },\n  methods: {\n    ...mapActions([\"CHANGE_SCREENSAVER_STATE\", \"CHANGE_SCREENSAVER_SHOWING\"]),\n  }\n}\n</script>\n\n<style>\n@font-face {\n  font-family: \"Helvetica\";\n  src: url(\"./assets/fonts/HelveticaNeueLTW1GCn.woff\") format(\"woff\"),\n  url(\"./assets/fonts/HelveticaNeueLTW1GCn.woff2\") format(\"woff2\");\n}\n\n@font-face {\n  font-family: \"Linotype\";\n  src: url(\"./assets/fonts/Linotype.woff\") format(\"woff\"),\n  url(\"./assets/fonts/Linotype.woff2\") format(\"woff2\");\n}\n\nbody{\n  margin: 0;\n  width: 1920px;\n  height: 1080px;\n  font-family: 'Linotype';\n  background-color: #000;\n}\np{\n  margin: 0;\n}\n\n.app{\n  height: 100vh;\n}\n\n::-webkit-scrollbar {\n  width: 0;\n}\n\n.opacity-enter-active {\n  animation-name: opacity-in;\n  animation-duration: 3s;\n}\n.opacity-leave-active {\n  animation-name: opacity-out;\n  animation-duration: 1s;\n}\n \n@keyframes opacity-in {\n  0% {\n    opacity: 0;\n  }\n  100% {\n    opacity: 1;\n  }\n}\n \n@keyframes opacity-out {\n  0% {\n    opacity: 1;\n  }\n  100% {\n    opacity: 0;\n  }\n}\n</style>\n"]}]}