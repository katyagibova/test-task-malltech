\"appbar\" \n      :class=\"{ active: showAppbar, nactive: hideAppbar }\"\n      v-if=\"SHOW_AUTHORS\">\n        <div class=\"appbar_title\">\n          <p>{{authors}}</p>\n          <img src=\"../assets/icons/button_close_full.svg\" alt=\"\" @click=\"closeAuthors\" style=\"height: 36px; cursor: pointer\">\n        </div>\n        <div class=\"appbar_divider\">\n        </div>\n        <div class=\"appbar_inner\">\n          <p class=\"appbar_inner_title\">\n            {{selectedCredits.photoTitle}}\n          </p>\n          <p class=\"appbar_inner_values\" v-for=\"i in selectedCredits.photoValues\" :key=\"i\">\n            {{i}}\n          </p>\n          <p class=\"appbar_inner_title\">\n            {{selectedCredits.videoTitle}}\n          </p>\n          <p class=\"appbar_inner_values\" v-for=\"i in selectedCredits.videoValues\" :key=\"i\">\n            {{i}}\n          </p>\n        </div>\n      </div>\n    </div>\n\n    <map name=\"Bookcase\">\n      <area shape=\"rect\" v-if=\"isClicked\" coords=\"250,190, 430,975\" @click=\"pushNamesBook\" style=\"cursor: pointer\">\n      <area shape=\"poly\" v-if=\"isClicked\" coords=\"421,290, 532,232, 865,910, 735,975\" @click=\"pushLiteratureBook\" style=\"cursor: pointer\">\n      <area shape=\"rect\" v-if=\"isClicked\" coords=\"880,808, 1700,975\" @click=\"pushPerestroikaBook\" style=\"cursor: pointer\">\n    </map>\n\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from \"vuex\";\n\nexport default {\n  name: 'MainScreen',\n  components: {\n  },\n  data(){\n    return {\n      imagePuth: '',\n      bookPuth: '',\n      lang: '',\n      isClicked: true,\n      authors: 'Авторы и правообладатели',\n      showAppbar: false,\n      hideAppbar: false,\n      credits: [\n        {\n          photoTitle: 'Фото:',\n          photoValues: ['Юрий Абрамочкин / РИА Новости', 'Игорь Виноградов / РИА Новости', 'Владимир Доценко / ИTAP-TACC', 'Юрий Иванов / РИА Новости', 'ИTAP-TACC','Владимир Ковригин, Александр Лесс / ИТАР-ТАСС', 'Рудольф Кучеров / РИА Новости', 'Александр Лесс / РИА Новости', 'Петр Малиновский / РИА Новости', 'Анатолий Морковкин / ИТАР-TACС', 'PИA Новости', 'Сергей Самохин / РИА Новости', 'Владимир Федоренко / РИА Новоети', 'Петр Шумов / Российский государственный архив литературы и искусства (РГАЛИ)', 'The Michigan Daily'],\n          videoTitle: 'Видео:',\n          videoValues: ['Виктор Виноградов / Государственный фонд телевизионных и радиопрограмм (Гостелерадиофонд)', 'Алексей Габрилович / Государственный фонд телевизионных и радиопрограмм (Гостелерадиофонд)', 'Лев Лурьев, Евгений Поротов / ТРК «Петербург - 5 канал»']\n        },\n        {\n          photoTitle: 'Photos:',\n          photoValues: ['Yuri Abramochkin / RIA Novosti', 'Igor Vinogradov / RIA Novosti', 'Vladimir Dotsenko / ITAR-TACC', 'Yuri Ivanov / RIA Novosti', 'ITAR-TACC','Vladimir Kovrigin, Alexander Less / ITAR-TASS', 'Rudolf Kucherov / RIA Novosti', 'Alexander Less / RIA Novosti', 'Pyotr Malinovsky / RIA Novosti', 'Anatoly Morkovkin / ITAR-TASS', 'RIA Novosti', 'Sergey Samokhin / RIA Novosti', 'Vladimir Fedorenko / RIA Novoeti', 'Peter Shumov / Russian State Archive of Literature and Art (RGALI)', 'The Michigan Daily'],\n          videoTitle: 'Videos:',\n          videoValues: ['Viktor Vinogradov / State Fund of Television and Radio Programs (Gosteleradiofond)', 'Alexey Gabrilovich / State Fund of Television and Radio Programs (Gosteleradiofond)', 'Lev Luryev, Evgeny Porotov / St. Petersburg - 5 channel']\n        },\n      ],\n      selectedCredits: {},\n      allImgs: ['../img/sequenceNamesBook.338b56cc.png', '../img/sequenceLiteratureBook.c9187ce6.png', '../img/sequencePerestroikaBook.eb8b3250.png', '../img/sequenceNamesBookEn.a51123ee.png', '../img/sequenceLiteratureBookEn.png', '../img/sequencePerestroikaBookEn.png']\n    }\n  },\n  watch: {\n    'SHOW_AUTHORS'(val){\n      this.showAppbar = val\n      this.hideAppbar = !val\n    },\n    'PAGE_LANG'(val){\n      this.lang = val\n      this.authors = this.lang === 'ru' ? 'Авторы и правообладатели' : 'Credits'\n      if(this.lang === 'ru'){\n        this.imagePuth = 'bookcase',\n        this.selectedCredits = this.credits[0]\n      } else {\n        this.imagePuth = 'bookcase_eng'\n        this.selectedCredits = this.credits[1]\n      }\n    }\n  },\n  mounted() {\n    this.lang = this.PAGE_LANG\n    this.authors = this.lang === 'ru' ? 'Авторы и правообладатели' : 'Credits'\n    if(this.lang === 'ru'){\n      this.imagePuth = 'bookcase',\n      this.selectedCredits = this.credits[0]\n    } else {\n      this.imagePuth = 'bookcase_eng'\n      this.selectedCredits = this.credits[1]\n    }\n  },\n  created(){\n  },\n  computed: {\n    ...mapGetters([\"SHOW_AUTHORS\", \"PAGE_LANG\"]),\n    imageSrc(){\n      if(!this.imagePuth){\n        return\n      }\n      if(this.lang === 'ru'){\n        return require(`../assets/sequence/${this.imagePuth}.svg`)\n      } else {\n        return require(`../assets/sequence_eng/${this.imagePuth}.svg`) \n      }\n    },\n    bookSrc(){\n      if(!this.bookPuth){\n        return\n      }\n      if(this.lang === 'ru'){\n        return require(`../assets/sequence/${this.bookPuth}.png`)\n      } else {\n        return require(`../assets/sequence_eng/${this.bookPuth}.png`) \n      }\n    },\n  },\n  methods: {\n    ...mapActions([\"CHANGE_SCREENSAVER_STATE\", \"CHANGE_AUTHORS_SHOWING\"]),\n\n    updateScreenSaver(){\n      this.CHANGE_SCREENSAVER_STATE(true)\n    },\n\n    closeAuthors(){\n      this.hideAppbar = true\n      setTimeout(() => {\n        this.CHANGE_AUTHORS_SHOWING(false)\n      }, 1000)\n    },\n\n    pushPerestroikaBook(){\n      this.isClicked = false\n      if(this.lang == 'ru'){\n        this.bookPuth = 'sequencePerestroikaBook'\n      } else {\n        this.bookPuth = 'sequencePerestroikaBookEn'\n      }\n      setTimeout(() => {\n        this.$router.push({name: 'BooksInfo', query: {id: 3}})\n        this.bookPuth = ''\n      }, 850)\n    },\n    pushNamesBook(){\n      this.isClicked = false\n      if(this.lang == 'ru'){\n        this.bookPuth = 'sequenceNamesBook'\n      } else {\n        this.bookPuth = 'sequenceNamesBookEn'\n      }\n      setTimeout(() => {\n        this.$router.push({name: 'BooksInfo', query: {id: 1}})\n        this.bookPuth = ''\n      }, 850)\n    },\n    pushLiteratureBook(){\n      this.isClicked = false\n      if(this.lang == 'ru'){\n        this.bookPuth = 'sequenceLiteratureBook'\n      } else {\n        this.bookPuth = 'sequenceLiteratureBookEn'\n      }\n      setTimeout(() => {\n        this.$router.push({name: 'BooksInfo', query: {id: 2}})\n        this.bookPuth = ''\n      }, 850)\n    }\n  }\n}\n</script>\n\n<style>\n.pano_img{\n  position: absolute;\n  top: 0;\n  right: 0;\n  z-index: 6;\n  width: 492px;\n  height: 654px;\n}\n\n.sequence_gif{\n  position: absolute;\n  top: 0;\n  left: 0;\n  z-index: 5;\n}\n\n.appbar{\n  position: absolute;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  width: 637px;\n  background: rgba(0, 0, 0, 0.9);\n  z-index: 11;\n}\n\n.appbar_title{\n  font-style: normal;\n  font-weight: 500;\n  font-size: 22px;\n  line-height: 26px;\n  color: #FFF;\n  display: flex;\n  justify-content: space-between;\n  padding: 35px 35px 3px;\n  align-items: end;\n}\n\n.appbar_divider{\n  width: 567px;\n  height: 2px;\n  background: #4B4B4B;\n  display: flex;\n  margin: 0 auto;\n}\n\n.active{\n  animation-duration: 1s;\n  animation-name: slidein;\n}\n\n.nactive{\n  animation-duration: 1s;\n  animation-name: slideout;\n}\n\n@keyframes slidein {\n  from {\n    opacity: 0;\n  }\n\n  to {\n    opacity: 1;\n  }\n}\n\n@keyframes slideout {\n  from {\n    opacity: 1;\n  }\n\n  to {\n    opacity: 0;\n  }\n}\n\n.appbar_inner{\n  margin: 29px 36px;\n}\n\n.appbar_inner_title{\n  font-style: normal;\n  font-weight: 500;\n  font-size: 21px;\n  line-height: 25px;\n  color: #91AA9B;\n  margin-bottom: 10px;\n}\n\n.appbar_inner_values{\n  font-style: normal;\n  font-weight: 500;\n  font-size: 21px;\n  line-height: 25px;\n  color: #D1D1D1;\n  margin-bottom: 10px;\n}\n</style>\n"]}]}